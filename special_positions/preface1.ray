/* point group representations using Rayshade 4.0
   preface file, contains color and object definitions
   for all symmetry elements, and is used to generate the
   special point movie
   Author: M. De Graef
   Date:   9/9/1995
*/


surface mirror        /* mirrored (silver) surface */
        ambient 0.14 0.14 0.14
        diffuse 0.01 0.01 0.01
        specular .8 .8 .8
        specpow 60
        reflect 1.

surface redmirror     /* red mirrored surface */
        ambient 0.12 0.00 0.00
        diffuse 0.15 0.00 0.00
        specular .9 .9 .9
        specpow 60
        reflect 1.

surface blumirror     /* blue mirrored surface */
        ambient 0.00 0.00 0.32
        diffuse 0.00 0.00 0.15
        specular .9 .9 .9
        specpow 60
        reflect 1.0

surface grnmirror     /* green mirrored surface */
        ambient 0.00 0.12 0.00
        diffuse 0.00 0.15 0.00
        specular .8 .8 .8
        specpow 60
        reflect 1.

surface ylwmirror     /* yellow mirrored surface */
        ambient 0.25 0.25 0.12
        diffuse 0.25 0.25 0.12
        specular .8 .8 .8
        specpow 60
        reflect 0.75

surface redflat       /* red surface */
        ambient 0.22 0.00 0.00
        diffuse 0.25 0.00 0.00
        specular .4 .4 .4
        specpow 20
        reflect 0.2

surface bluflat       /* blue surface */
        ambient 0.04 0.06 0.22
        diffuse 0.05 0.08 0.25
        specular .4 .4 .4
        specpow 20
        reflect 0.2

surface grnflat       /* green surface */
        ambient 0.00 0.22 0.00
        diffuse 0.00 0.25 0.00
        specular .4 .4 .4
        specpow 20
        reflect 0.2

surface ylwflat       /* yellow surface */
        ambient 0.25 0.25 0.12
        diffuse 0.25 0.25 0.12
        specular .4 .4 .4
        specpow 20
        reflect 0.2

surface white
        ambient 0.06 0.06 0.06
        diffuse 0.50 0.50 0.50
        specular 0.7 0.7 0.7
        specpow 38
        reflect 0.5

surface black
        ambient 0.00 0.00 0.00
        diffuse 0.0 0.0 0.0
        specular 0.2 0.2 0.2
        specpow 35
        reflect 0.4


plane white 0 0 -200 0 0 1


name mirrorplane
list
	poly mirror  1.0 1.0 0.005  1.0 -1.0 0.005  -1.0 -1.0 0.005  -1.0 1.0 0.005
	poly mirror  1.0 1.0 -0.005  1.0 -1.0 -0.005  -1.0 -1.0 -0.005  -1.0 1.0 -0.005
	poly black   1.0 1.0 0.005  1.0 1.0 -0.005  1.0 -1.0 -0.005  1.0 -1.0 0.005
	poly black   1.0 -1.0 0.005  1.0 -1.0 -0.005  -1.0 -1.0 -0.005  -1.0 -1.0 0.005
	poly black  -1.0 -1.0 0.005  -1.0 -1.0 -0.005  -1.0  1.0 -0.005  -1.0  1.0 0.005 
	poly black  -1.0  1.0 0.005  -1.0  1.0 -0.005  1.0  1.0 -0.005  1.0  1.0 0.005 
end

