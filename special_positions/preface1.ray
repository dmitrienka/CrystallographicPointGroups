frames 9

define length 8	/* # of frames of movement */

/* first from the fourfold axis out to a general point  */
define line_onex (linear(0, 0.0, length , 0.65))
define line_oney (linear(0, 0.0, length , 0.15))
define px ( line_onex )
define py ( line_oney )
define pz 0.40
define mpx (-px)
define mpy (-py)
define mpz (-pz) 


